{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "mint",
    "name": "Mint Starter Kit",
    "colors": {
        "primary": "#FF2157",
        "light": "#FF637E",
        "dark": "#EC0040"
    },
    "favicon": "/favicon.svg",
    "navigation": {
        "tabs": [
            {
                "tab": "Referenz",
                "groups": [
                    {
                        "group": "Erste Schritte",
                        "pages": ["start/unternehmen", "start/leistungen", "start/trainingsorte", "start/import"]
                    },
                    {
                        "group": "Hundeschule",
                        "pages": []
                    }
                ]
            },
            {
                "tab": "API Reference",
                "groups": [
                    {
                        "group": "API Documentation",
                        "pages": ["api-reference/introduction"]
                    },
                    {
                        "group": "Endpoint Examples",
                        "pages": ["api-reference/endpoint/get", "api-reference/endpoint/create", "api-reference/endpoint/delete", "api-reference/endpoint/webhook"]
                    }
                ]
            }
        ],
        "global": {
            "languages": [
                {
                    "language": "de",
                    "default": true,
                    "href": "https://123hundeschule.de"
                }
            ],
            "anchors": [
                {
                    "anchor": "FAQ",
                    "href": "https://faq.123hundeschule.de",
                    "icon": "book-open-cover"
                },
                {
                    "anchor": "Community",
                    "href": "https://123hundeschule.de/",
                    "icon": "slack"
                },
                {
                    "anchor": "Blog",
                    "href": "https://123hundeschule.de/blog",
                    "icon": "newspaper"
                }
            ]
        }
    },
    "logo": {
        "light": "/logo/logo.svg",
        "dark": "/logo/logo.svg"
    },
    "navbar": {
        "links": [
            {
                "label": "Support",
                "href": "mailto:support@123hundeschule.de"
            }
        ],
        "primary": {
            "type": "button",
            "label": "Website",
            "href": "https://123hundeschule.de"
        }
    },
    "footer": {
        "socials": {}
    }
}
